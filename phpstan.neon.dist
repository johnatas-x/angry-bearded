includes:
  - phpstan-drupal.neon

parameters:
    level: 10
    reportUnmatchedIgnoredErrors: false
    treatPhpDocTypesAsCertain: false
    inferPrivatePropertyTypeFromConstructor: true
    scanDirectories:
      - vendor/drush/drush
    paths:
      - web/modules/custom
      - web/profiles/custom
      - web/themes/custom
